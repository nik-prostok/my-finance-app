{
  "name": "react-webpack5-ts-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate project with React, React Router, Webpack 5, Babel, Typescript, Eslint",
  "main": "index.js",
  "repository": "https://github.com/SergejSKoreshkov/fintor.git",
  "author": "SergejSKoreshkov@gmail.com",
  "license": "MIT",
  "scripts": {
    "serve": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "yarn lint --fix",
    "deploy:github": "NODE_ENV=github webpack --mode=production -o docs && git add . && git commit -m 'DEPLOY TO GITHUB PAGES' && git push origin main"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "path": "^0.12.7",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "yarn-upgrade-all": "^0.7.1"
  },
  "packageManager": "yarn@3.2.2"
}
